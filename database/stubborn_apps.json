        "HKEY_LOCAL_MACHINE\\SOFTWARE\\TeamViewer",
        "HKEY_LOCAL_MACHINE\\SOFTWARE\\WOW6432Node\\TeamViewer"
      ]
    },
    {
      "name": "Dropbox",
      "publisher": "Dropbox, Inc.",
      "patterns": ["Dropbox"],
      "processes_to_kill": [
        "Dropbox.exe",
        "DropboxUpdate.exe"
      ],
      "services_to_stop": [
        "dbupdate",
        "dbupdatem"
      ],
      "additional_paths": [
        "%APPDATA%\\Dropbox",
        "%LOCALAPPDATA%\\Dropbox",
        "%PROGRAMDATA%\\Dropbox"
      ],
      "registry_keys": [
        "HKEY_CURRENT_USER\\Software\\Dropbox",
        "HKEY_LOCAL_MACHINE\\SOFTWARE\\Dropbox",
        "HKEY_LOCAL_MACHINE\\SOFTWARE\\WOW6432Node\\Dropbox"
      ]
    },
    {
      "name": "OneDrive",
      "publisher": "Microsoft Corporation",
      "patterns": ["OneDrive", "Microsoft OneDrive"],
      "processes_to_kill": [
        "OneDrive.exe",
        "FileCoAuth.exe"
      ],
      "additional_paths": [
        "%LOCALAPPDATA%\\Microsoft\\OneDrive",
        "%PROGRAMDATA%\\Microsoft OneDrive"
      ],
      "registry_keys": [
        "HKEY_CURRENT_USER\\Software\\Microsoft\\OneDrive",
        "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\OneDrive",
        "HKEY_LOCAL_MACHINE\\SOFTWARE\\WOW6432Node\\Microsoft\\OneDrive"
